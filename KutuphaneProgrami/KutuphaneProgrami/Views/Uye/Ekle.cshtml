
@{
    ViewBag.Title = "Uye Ekle";
}

<h2 style="text-align:center">UYE EKLE</h2>

<form action="#">

    <!-- Adı -->
    <div class="form-group">
        <h4>Adı</h4>
        <div>
            <input id="Ad" type="text" placeholder="Adınızı Giriniz">
        </div>
    </div>
    <!--  Soyad -->
    <div class="form-group">
        <h4>Soyad</h4>
        <div>
            <input id="Soyad" type="text" placeholder="Soyadınızı Giriniz">
        </div>
    </div>
    <!-- Tc -->
    <div class="form-group">
        <h4>Tc Kimlik No</h4>
        <div>
            <input id="Tc" type="text" placeholder="Tc Kimlik No Giriniz">
        </div>
    </div>
    <!-- Tel -->
    <div class="form-group">
        <h4>Tel</h4>
        <div>
            <input id="Tel" type="text" placeholder="Telefon Numaranızı Giriniz">
        </div>
    </div>
    <!--  KAYDET -->
    <div class="form-group">
        <div class="controls">
            <input type="button" id="uyeKaydet" value="Ekle">
        </div>
    </div>

</form>

